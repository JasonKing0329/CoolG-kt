<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_title_a"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:gravity="center"
            android:text="Group A"
            android:textSize="30dp"
            android:textStyle="bold|italic"
            android:textColor="#D84242"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv_title_b"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:id="@+id/tv_title_b"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:gravity="center"
            android:text="Group B"
            android:textSize="30dp"
            android:textStyle="bold|italic"
            android:textColor="#21BED3"
            app:layout_constraintStart_toEndOf="@+id/tv_title_a"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_top1_a"
            android:layout_width="0dp"
            android:layout_height="@dimen/play_order_height_cell1"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="@+id/tv_title_a"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_title_a"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed1_a"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top1_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top1_a"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top1_b"
            android:layout_width="0dp"
            android:layout_height="@dimen/play_order_height_cell1"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tv_title_b"
            app:layout_constraintTop_toBottomOf="@+id/tv_title_b"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed1_b"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top1_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top1_b"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top2_a"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginTop="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/iv_top3_a"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_top1_a"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed2_a"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top2_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top2_a"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top3_a"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginStart="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="@+id/tv_title_a"
            app:layout_constraintStart_toEndOf="@+id/iv_top2_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top2_a"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed3_a"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top3_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top3_a"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top4_a"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginTop="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/iv_top5_a"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_top3_a"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed4_a"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top4_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top4_a"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top5_a"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginStart="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="@+id/tv_title_a"
            app:layout_constraintStart_toEndOf="@+id/iv_top4_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top4_a"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed5_a"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top5_a"
            app:layout_constraintTop_toTopOf="@+id/iv_top5_a"
            tools:text="[5]" />

        <ImageView
            android:id="@+id/iv_top2_b"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginTop="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/iv_top3_b"
            app:layout_constraintStart_toStartOf="@+id/tv_title_b"
            app:layout_constraintTop_toBottomOf="@+id/iv_top1_b"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed2_b"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top2_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top2_b"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top3_b"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginStart="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iv_top2_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top2_b"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed3_b"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top3_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top3_b"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top4_b"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginTop="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/iv_top5_b"
            app:layout_constraintStart_toStartOf="@+id/tv_title_b"
            app:layout_constraintTop_toBottomOf="@+id/iv_top3_b"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed4_b"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top4_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top4_b"
            tools:text="[5]" />
        <ImageView
            android:id="@+id/iv_top5_b"
            android:layout_width="0dp"
            android:layout_height="@dimen/img_height_cell1of4"
            android:layout_marginStart="1dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iv_top4_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top4_b"
            app:srcCompat="@drawable/def_small" />
        <TextView
            android:id="@+id/tv_seed5_b"
            style="@style/TvMatchFinalSeed"
            app:layout_constraintStart_toStartOf="@+id/iv_top5_b"
            app:layout_constraintTop_toTopOf="@+id/iv_top5_b"
            tools:text="[5]" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/group_title_a"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:paddingBottom="8dp"
            app:layout_constraintTop_toBottomOf="@id/iv_top4_a"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_title_a">
            <TextView
                android:id="@+id/tv_rank_title_a"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="Rank"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="Record"
                android:textSize="12dp"
                app:layout_constraintStart_toEndOf="@+id/tv_rank_title_a"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="Match"
                android:textSize="12dp"
                android:gravity="right"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/group_title_b"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingBottom="8dp"
            app:layout_constraintTop_toTopOf="@id/group_title_a"
            app:layout_constraintStart_toStartOf="@+id/tv_title_b"
            app:layout_constraintEnd_toEndOf="parent">
            <TextView
                android:id="@+id/tv_rank_title_b"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="Rank"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="Record"
                android:textSize="12dp"
                app:layout_constraintStart_toEndOf="@+id/tv_rank_title_b"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="Match"
                android:textSize="12dp"
                android:gravity="right"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
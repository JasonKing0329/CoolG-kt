<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.king.app.coolg_kt.view.widget.chart.LineChart
            android:id="@+id/chart_rank"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingTop="10dp"
            android:paddingRight="16dp"
            app:axisXTextSize="6dp"
            app:axisYTextSize="8dp"
            app:axisYTextWidth="30dp"
            app:axisXExtendLineWidth="10dp"
            app:minXCellWidth="6dp"
            app:valueTextSize="6dp"
            app:lineColor="#3399ff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>